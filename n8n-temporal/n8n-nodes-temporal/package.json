{
  "name": "n8n-nodes-temporal",
  "version": "1.0.0",
  "description": "n8n nodes for Temporal workflow orchestration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "temporal",
    "workflow"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ewnd9/code-research",
  "author": {
    "name": "n8n-temporal-research",
    "email": "research@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/code-research.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TemporalApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Temporal/Temporal.node.js",
      "dist/nodes/Temporal/TemporalContentGeneration.node.js",
      "dist/nodes/Temporal/TemporalContentPipeline.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "tslint": "^6.1.3",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@temporalio/client": "^1.11.3",
    "n8n-core": "*"
  }
}
