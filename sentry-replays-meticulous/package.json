{
  "name": "sentry-replays-meticulous",
  "version": "0.1.0",
  "description": "Build a meticulous.ai-style automated testing system using Sentry's session replay data",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun run src/example.ts",
    "example": "bun run src/example.ts",
    "client": "bun run src/sentry-client.ts",
    "analyzer": "bun run src/replay-analyzer.ts",
    "test": "bun test",
    "test:playwright": "bunx playwright test",
    "clean": "rm -rf dist generated-tests node_modules",
    "install": "bun install"
  },
  "keywords": [
    "sentry",
    "replay",
    "testing",
    "automation",
    "playwright",
    "e2e",
    "bun"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "bun-types": "latest"
  }
}
